# vim: ft=sh

. ~/.tq84-dotfile-sequence .profile

export TZ=Europe/Zurich
export EDITOR=gvim

echo $(date) .profile: \$BASH=$BASH >> ~/.dotfile-sequence.log
if [ "$BASH" ] && [ -f ~/.bashrc ]; then
    echo $(date) .profile ~/.bashrc exists, so sourcing it >> ~/.dotfile-sequence.log
	. ~/.bashrc
fi

true
